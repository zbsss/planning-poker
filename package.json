{
  "name": "next-graphql-template",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "build": "next build",
    "nexus-typegen": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' --transpile-only ./graphql/schema.ts",
    "codegen": "graphql-codegen",
    "dev:all": "concurrently -r \"next dev\" \"graphql-codegen --watch \"generated/schema.graphql\"\""
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@auth0/nextjs-auth0": "^1.9.1",
    "@prisma/client": "^4.2.1",
    "apollo-server-micro": "^3.10.2",
    "express": "^4.18.1",
    "graphql": "^16.5.0",
    "graphql-relay": "^0.10.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.10.1",
    "jsonwebtoken": "^8.5.1",
    "micro-cors": "^0.1.1",
    "next": "12.2.5",
    "nexus": "^1.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.36.1",
    "ws": "^8.8.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.12.0",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/typescript-react-apollo": "^3.3.3",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.4",
    "@types/micro-cors": "^0.1.2",
    "@types/node": "18.7.3",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "autoprefixer": "^10.4.8",
    "concurrently": "^7.3.0",
    "eslint": "8.22.0",
    "eslint-config-next": "12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prisma": "^4.2.1",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "typescript": "4.7.4"
  }
}
